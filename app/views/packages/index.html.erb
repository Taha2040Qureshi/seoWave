<style> 
.dela {
    
     border-bottom: 6px solid red;
     border-top: 6px solid red;
    position: relative;
    -webkit-animation: mymove 5s infinite; /* Safari 4.0 - 8.0 */
    -webkit-animation-delay: 1s; /* Safari 4.0 - 8.0 */
    animation: mymove 5s infinite;
    animation-delay: 1s;

}
img{
         border-radius: 50%;
}
.rotate {
    border-radius: 50%;
    transition: all 0.5s ease-in-out;
    
}
.rotate:hover{
    opacity: 1;
    transform: rotate(360deg);
}

@keyframes mymove {
    from {left : 0px; border-bottom:6px solid black; border-top:6px solid black;}
    to {  left:10px;  border-bottom: 6px solid gray;  border-top: 6px solid gray;}
}
</style>

